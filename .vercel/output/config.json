{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "render"
		},
		{
			"src": "^\\/$",
			"dest": "render"
		},
		{
			"src": "^\\/de\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/fr\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/it\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/pl\\/?$",
			"dest": "render"
		},
		{
			"src": "^\\/pt\\/?$",
			"dest": "render"
		}
	]
}